config bot 'main'
	option enabled '1'
	option start_timeout_sec '30'
	option token ''
	option chat_id ''
	option db_path '/etc/newdevnotify/known_clients.db'
	option main_label 'Основная сеть'
	option guest_label 'Гостевая сеть'
	option main_repeat_after_sec '0'
	option guest_repeat_after_sec '86400'
	option template '<b>Роутер</b>\n<u>Подключено новое устройство</u> к <b>$NETNAME</b>\nДата: $DATE\nИмя: $HOST\nMAC: $MAC\nIP: $IP'

